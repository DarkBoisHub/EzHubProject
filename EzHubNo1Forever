local key = _G.EzKey
local check = "https://github.com/DarkBoisHub/EzHubProject/blob/main/check.php?key=" .. key
if game:HttpGet(check) == "Whitelisted" then
loadstring(game:HttpGet("https://raw.githubusercontent.com/DarkBoisHub/EzHubProject/main/script.txt"))()
else
game.Players.LocalPlayer:Kick("คีย์ผิด!!!")
end
